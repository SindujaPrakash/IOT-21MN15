<?xml version="1.0" encoding="UTF-8"?>
<LWM2M  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
<Object ObjectType="MODefinition">
	<Name>ParkingSpot</Name>
	<Description1>Self defined object for parking spot device</Description1>
	<ObjectID>32700</ObjectID>
	<MultipleInstances>Single</MultipleInstances>
	<Mandatory>Mandatory</Mandatory>
	<Resources>
		<Item ID="32800">
				<Name>ParkingSpotID</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<RangeEnumeration></RangeEnumeration>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<Units>Defined by “Units” resource.</Units>
				<Description>The ID number of the parking spot</Description>
			</Item>
		<Item ID="32801">
				<Name>ParkingSpotState</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<RangeEnumeration>Occupied, Free, Reserved</RangeEnumeration>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<Units></Units>
				<Description>The state of the parking spot whether it is occupied, free or reserved</Description>
			</Item>
		<Item ID="32802">
				<Name>VehicleID</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<RangeEnumeration></RangeEnumeration>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<Units></Units>
				<Description>Vehicle id that is currently reserving or occupying the parking spot</Description>
			</Item>
		<Item ID="32803">
				<Name>BillingRate</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<RangeEnumeration></RangeEnumeration>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<Units>EUR</Units>
				<Description>Billing rate of the parking spot per minute</Description>
			</Item>
		<Item ID="32804">
				<Name>Vehicle Plate Image</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<RangeEnumeration></RangeEnumeration>
				<Mandatory>Optional</Mandatory>
				<Type>Opaque</Type>
				<Units></Units>
				<Description>A valid image of the license plate of the parked vehicle</Description>
			</Item>
	</Resources>
</Object>
</LWM2M>